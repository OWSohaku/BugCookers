}

@media screen and (max-width: 768px) {
  .linkAdressDisable:link, .linkAdressDisable:hover{
    color: blue;
    cursor: default;
    text-decoration: underline;
    pointer-events: auto;
  }

  .linkAdressDisable:visited {
    color: purple;
    cursor: auto;
  }
}



